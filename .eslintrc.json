{
  "extends": "next/core-web-vitals",
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "array-bracket-spacing": ["error", "always"],
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "always",
      "asyncArrow": "always"
    }],
    "space-in-parens": ["error", "always"]
  }
}
